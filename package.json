{
  "name": "typoa",
  "version": "1.0.0-alpha.65",
  "author": "Eywek <contact@eywek.fr>",
  "description": "Help you generate OpenAPI definitions via Typescript",
  "homepage": "https://github.com/Eywek/typoa#readme",
  "main": "build/index.js",
  "files": [
    "build/"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rm -rf build",
    "build": "tsc --project ./tsconfig.build.json",
    "postbuild": "mkdir build/template/ && cp src/template/express.ts.hbs build/template/express.ts.hbs",
    "test": "bun test --coverage --coverage-reporter=text --coverage-reporter=lcov",
    "lint": "eslint --fix .",
    "prettier": "prettier --write ."
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.12",
    "@types/express": "5.0.6",
    "@types/node": "25.0.9",
    "@types/supertest": "6.0.3",
    "@types/yamljs": "0.2.34",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "body-parser": "1.19.0",
    "eslint": "9.39.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "62.0.0",
    "express": "4.17.1",
    "openapi-types": "12.1.3",
    "prettier": "3.8.0",
    "supertest": "7.2.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0"
  },
  "dependencies": {
    "debug": "4.4.3",
    "glob": "13.0.0",
    "handlebars": "4.7.8",
    "ts-morph": "27.0.2",
    "yamljs": "0.3.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
