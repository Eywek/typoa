{
  "compilerOptions": {
    "rootDirs": [ "./" ],
    "experimentalDecorators": true,
    "module": "commonjs",
    "declaration": true,
    "importHelpers": false,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2018",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "lib": [
      "es2019"
    ]
  }
}
